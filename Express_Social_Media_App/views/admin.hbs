<h1>{{title}}</h1>
<h1>Users:</h1>
<ul>
    {{#each users}}
    <li>
        <form method="POST" action="/users/admin/delete/{{UserId}} ">
            <a href="/users/admin/editUser/{{UserId}}">{{FirstName}} {{LastName}}</a>
            <input type="submit" value="Delete User">
        </form>
    </li>
    {{/each}}
</ul>
<h4>Click to View User's Profile</h4>
<br>
<a href="/users/profile">Back</a>

<a href="/users/logout">Logout</a>